---
export interface Properties {
  className?: string;
  size?: number;
}
const { size = 1, className = '' } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  width={`${size}em`}
  height={`${size}em`}
  class:list={className}
  fill="currentColor">
  <slot />
</svg>
